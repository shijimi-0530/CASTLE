<h1>New Castles</h1>

 <%= form_with model: @castle do |f| %>
   <%= f.text_field :name %>
   <%= f.text_field :content %>
   <%= f.select :region, Castle.region_areas.keys, {} %>
   <%= f.select :prefecture, Castle.prefectures_areas.keys, {} %>
   <%= f.text_field :lord %>
   <%= f.text_field :construction_year %>
   <%= f.text_field :image %>
   <%= f.submit "Create" %>
 <% end %>
 
 <%= link_to "Back", castles_path %>
