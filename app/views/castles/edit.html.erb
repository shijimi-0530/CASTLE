 <h1>Update Castles</h1>
 
 <%= form_with model: @castle do |f| %>
   <%= f.text_field :name %>
   <%= f.text_field :content %>
   <%= f.text_area :region %>
   <%= f.text_area :prefectures %>
   <%= f.text_field :lord %>
   <%= f.text_field :construction_year %>
   <%= f.text_field :image %>
   <%= f.submit "Update" %>
 <% end %>
 
 <%= link_to "Back", castles_path %>

